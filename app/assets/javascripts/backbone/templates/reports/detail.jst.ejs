        </div>
        <div class="form-group">
          <label class="col-sm-2 control-label">Resident Status</label>
          <div class="col-sm-8">
            <select class="form-control selectpicker" name="status" multiple title='Select Status'>
              <% for (var i=0, s; s=statuses[i]; i++) { %>
        	      <option value="<%= s %>"> <%= s %> </option>
              <% } %>
            </select>
          </div>
        </div>
        <div class="form-group">
          <label class="col-sm-2 control-label">Rental Type</label>
          <div class="col-sm-8">
            <select class="form-control selectpicker" name="rental_type" multiple title='Select Type'>
