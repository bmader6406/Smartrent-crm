<% if(!App.vars.isPropertyPage) { %>
  <div class="col-md-4">
    <div class="card">
      <h3>Contact Info </h3>
      <ul class="list-unstyled">
        <li>
          <label>Address:</label>
          <%= street %> <%= city %>, <%= state %> <%= zip %>
        </li>
        <li>
          <label>Gender:</label>
          <%= gender %>
        </li>
        <li>
          <label>Birthday:</label>
          <%= birthday %>
        </li>
        <li>
          <label>SSN#:</label>
          <%= last4_ssn %>
        </li>
        <li>
          <label>Cell Phone:</label>
          <%= cell_phone %>
        </li>
        <li>
          <label>Home Phone:</label>
          <%= home_phone %>
        </li>
        <li>
          <label>Work Phone:</label>
          <%= work_phone %>
        </li>
      </ul>
    </div>
  </div>

<% } else { %>
  <div class="col-md-4">
    <div class="card">
      <h3>
        Contact Info
      </h3>
      <ul class="list-unstyled">
        <li>
          <label>Address:</label>
          <%= street %> <%= city %>, <%= state %> <%= zip %>
        </li>
        <li>
          <label>Cell Phone:</label>
          <%= cell_phone %>
        </li>
        <li>
          <label>Home Phone:</label>
          <%= home_phone %>
        </li>
        <li>
          <label>Work Phone:</label>
          <%= work_phone %>
        </li>
      </ul>
    </div>
  </div>

  <div class="col-md-4">
    <div class="card">
      <h3>
        Lease Info
      </h3>
      <ul class="list-unstyled">
        <li>
          <label>Unit #:</label>
          <%= unit_code %>
        </li>
        <li>
          <label>Contract Signing Date:</label>
          <%= unit.signing_date %>
        </li>
        <li>
          <label>Move-in Date:</label>
          <%= unit.move_in %>
        </li>
        <li>
          <label>Move-out Date:</label>
          <%= unit.move_out %>
        </li>
        <li>
          <label>Rent:</label>
          <%= unit.rent %>
        </li>
        <li>
          <label>Status:</label>
          <%= unit.status %>
        </li>
        <li>
          <label>Type:</label>
          <%= unit.type %>
        </li>
        <li class="heading"> Vehicle Information</li>
        <li>
          <label>Number of Vehicles:</label>
          <%= unit.vehicles_count %>
        </li>
        <% if (unit.vehicles_count > 0 && unit.vehicle1) { %>
          <li>
            <label>Vehicle 1</label> <br/>
            <% if(unit.vehicle1) { %>
              - Make/Model/Year: <%= unit.vehicle1 %> <br/>
            <% } %>
            
            <% if(unit.license1) { %>
              - License Plate #: <%= unit.license1 %> <br/>
            <% } %>
            
            <% if(unit.badge_number_1) { %>
              - Badge #: <%= unit.badge_number_1 %>
            <% } %>
          </li>
        <% } %>

        <% if(unit.vehicles_count > 1 && unit.vehicle2) { %>
          <li>
            <label>Vehicle 2</label> <br/>
            <% if(unit.vehicle2) { %>
              - Make/Model/Year: <%= unit.vehicle2 %> <br/>
            <% } %>
            
            <% if(unit.license2) { %>
              - License Plate #: <%= unit.license2 %> <br/>
            <% } %>
            
            <% if(unit.badge_number_2) { %>
              - Badge #: <%= unit.badge_number_2 %>
            <% } %>
          </li>
        <% } %>

        <% if(unit.vehicles_count > 2 && unit.vehicle3) { %>
          <li>
            <label>Vehicle 3</label> <br/>
            <% if(unit.vehicle3) { %>
              - Make/Model/Year: <%= unit.vehicle3 %> <br/>
            <% } %>
            
            <% if(unit.license3) { %>
              - License Plate #: <%= unit.license3 %> <br/>
            <% } %>
            
            <% if(unit.badge_number_3) { %>
              - Badge #: <%= unit.badge_number_3 %>
            <% } %>
          </li>
        <% } %>
        <% if(unit.vehicles_count > 3 && unit.vehicle4) { %>
          <li>
            <label>Vehicle 4</label> <br/>
            <% if(unit.vehicle4) { %>
              - Make/Model/Year: <%= unit.vehicle4 %> <br/>
            <% } %>
            
            <% if(unit.license4) { %>
              - License Plate #: <%= unit.license4 %> <br/>
            <% } %>
            
            <% if(unit.badge_number_4) { %>
              - Badge #: <%= unit.badge_number_4 %>
            <% } %>
          </li>
        <% } %>
        <% if(unit.vehicles_count > 4 && unit.vehicle5) { %>
          <li>
            <label>Vehicle 5</label> <br/>
            <% if(unit.vehicle5) { %>
              - Make/Model/Year: <%= unit.vehicle5 %> <br/>
            <% } %>
            
            <% if(unit.license5) { %>
              - License Plate #: <%= unit.license5 %> <br/>
            <% } %>
            
            <% if(unit.badge_number_5) { %>
              - Badge #: <%= unit.badge_number_5 %>
            <% } %>
          </li>
        <% } %>
          <li class="heading"> Pet Information</li>
          <li>
            <label>Pets Count:</label>
            <%= unit.pets_count %>
          </li>
          <% if (unit.pets_count >= 1 && unit.pet_1_type) { %>
            <li>
              <label>Pet 1</label> <br/>
              <% if(unit.pet_1_type) { %>
                - Type: <%= unit.pet_1_type %> <br/>
              <% } %>
            
              <% if(unit.pet_1_breed){ %>
                - Breed: <%= unit.pet_1_breed %> <br/>
              <% } %>
            
              <% if(unit.pet_1_name){ %>
                - Name: <%= unit.pet_1_name %> <br/>
              <% } %>
            </li>
          <% } %>
          
          <% if (unit.pets_count >= 2 && unit.pet_2_type) { %>
            <li>
              <label>Pet 2</label> <br/>
              <% if(unit.pet_2_type) { %>
                - Type: <%= unit.pet_2_type %> <br/>
              <% } %>
            
              <% if(unit.pet_2_breed){ %>
                - Breed: <%= unit.pet_2_breed %> <br/>
              <% } %>
            
              <% if(unit.pet_2_name){ %>
                - Name: <%= unit.pet_2_name %> <br/>
              <% } %>
            </li>
          <% } %>
          
          <% if (unit.pets_count >= 3 && unit.pet_3_type) { %>
            <li>
              <label>Pet 3</label> <br/>
              <% if(unit.pet_3_type) { %>
                - Type: <%= unit.pet_3_type %> <br/>
              <% } %>
            
              <% if(unit.pet_3_breed){ %>
                - Breed: <%= unit.pet_3_breed %> <br/>
              <% } %>
            
              <% if(unit.pet_3_name){ %>
                - Name: <%= unit.pet_3_name %> <br/>
              <% } %>
            </li>
          <% } %>
      </ul>
    </div>
  </div>

  <% if (App.vars.isCrm) { %>
    <div class="col-md-4">
      <div class="card">
        <h3>
          Demographics
        </h3>
        <ul class="list-unstyled">
          <li class="heading"> Occupant </li>
          <li>
            <label>Gender:</label>
            <%= gender %>
          </li>
          <li>
            <label>Birthday:</label>
            <%= birthday %>
          </li>
          <li>
            <label>SSN:</label>
            <%= ssn %>
          </li>
          <li>
            <label>Household Size:</label>
            <%= unit.household_size %>
          </li>
          <li>
            <label>Household Status:</label>
            <%= unit.household_status %>
          </li>

          <li class="heading"> Previous Residence</li>
          <li>
            <label>Moving From:</label>
            <%= unit.moving_from %>
          </li>


          <li class="heading"> Employment</li>
          <li>
            <label>Occupation Type:</label>
            <%= unit.occupation_type %>
          </li>
          <li>
            <label>Employer:</label>
            <%= unit.employer %>
          </li>
          <li>
            <label>Employer City:</label>
            <%= unit.employer_city %>
          </li>
          <li>
            <label>Employer State:</label>
            <%= unit.employer_state %>
          </li>
          <li>
            <label>Annual Income:</label>
            <%= unit.annual_income %>
          </li>
          <li>
            <label>Minutes to Work:</label>
            <%= unit.minutes_to_work %>
          </li>
          <li>
            <label>Transportation to Work:</label>
            <%= unit.transportation_to_work %>
          </li>
        </ul>
      </div>
    </div>
  <% } %>
  <% if ( App.vars.isSmartrent && smartrent ) { %>
    <div class="col-md-4">
      <div class="card">
        <h3>
          SmartRent
        </h3>
        <ul class="list-unstyled smartrent-info"> <!-- will be loaded when the user view the resident detail -->
          Loading...
          <li>
            <a href="#" class="btn btn-primary btn-lg view-smartrent"> View Details</a>
          </li>
        </ul>
      </div>
    </div>
  <% } %>
<% } %>
