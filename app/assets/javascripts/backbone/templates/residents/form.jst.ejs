<form class="form-horizontal resident-form">
  <div class="panel-group accordion">

    <% if(App.vars.isPropertyPage){ %> <!-- property create, update form -->
      <div class="panel panel-default">
        <div class="panel-heading">
          <h4 class="panel-title">
            <i class="fa fa-minus"></i> <i class="fa fa-plus"></i>
            <a data-toggle="collapse" data-parent=".accordion" href="#contact-info"> Resident Information </a>
          </h4>
        </div>
        <div id="contact-info" class="panel-collapse collapse">
          <div class="panel-body">
            <div class="row">
              <div class="col-md-6">
                <div class="heading"> Basic Information </div>
                <div data-fields="full_name,email,primary_phone"> </div>

                <div class="heading"> Contact Information </div>
                <div data-fields="street,city,state,zip,cell_phone,home_phone,work_phone"> </div>
              </div>
              <div class="col-md-6">
                <div class="heading"> Lease Information </div>
                <div data-fields="unit_id,signing_date,move_in,move_out,rent,status,type"> </div>
                
                <div data-fields="pets_count"> </div>
                
                <div class="form-group pet-1">
                  <label class="col-sm-4 control-label">
                    Pet 1. Type
                  </label>
                 <div class="col-sm-8">
                    <div class="row">
                      <div class="col-sm-4" data-editors="pet_1_type" title="Pet Type"></div>
                      <div class="col-sm-4" data-editors="pet_1_breed" title="Pet Breed"></div>
                      <div class="col-sm-4" data-editors="pet_1_name" title="Pet Name"></div>
                    </div>
                  </div>
                </div>
                <div class="form-group pet-2">
                  <label class="col-sm-4 control-label">
                    Pet 2. Type
                  </label>
                  <div class="col-sm-8">
                    <div class="row">
                      <div class="col-sm-4" data-editors="pet_2_type" title="Pet Type"></div>
                      <div class="col-sm-4" data-editors="pet_2_breed" title="Pet Breed"></div>
                      <div class="col-sm-4" data-editors="pet_2_name" title="Pet Name"></div>
                    </div>
                  </div>
                </div>
                <div class="form-group pet-3">
                  <label class="col-sm-4 control-label">
                    Pet 3. Type
                  </label>
                  <div class="col-sm-8">
                    <div class="row">
                      <div class="col-sm-4" data-editors="pet_3_type" title="Pet Type"></div>
                      <div class="col-sm-4" data-editors="pet_3_breed" title="Pet Breed"></div>
                      <div class="col-sm-4" data-editors="pet_3_name" title="Pet Name"></div>
                    </div>
                  </div>
                </div>
                
                
                <div data-fields="vehicles_count"> </div>
                <div class="vehicle-detail">
                  <div class="form-group vehicle-1">
                    <label for="vehicle1" class="col-sm-4 control-label">
                      Vehicle 1. Make/Model/Year
                    </label>
                    <div class="col-sm-8">
                      <div class="row">
                        <div class="col-sm-4" data-editors="vehicle1" title="Vehicle Make/Model/Year"></div>
                        <div class="col-sm-4" data-editors="license1" title="License Plate #"></div>
                        <div class="col-sm-4" data-editors="badge_number_1" title="Badge #"></div>
                      </div>
                    </div>
                  </div>

                  <div class="form-group vehicle-2">
                    <label for="vehicle2" class="col-sm-4 control-label">
                      Vehicle 2. Make/Model/Year
                    </label>
                    <div class="col-sm-8">
                      <div class="row">
                        <div class="col-sm-4" data-editors="vehicle2" title="Vehicle Make/Model/Year"></div>
                        <div class="col-sm-4" data-editors="license2" title="License Plate #"></div>
                        <div class="col-sm-4" data-editors="badge_number_2" title="Badge #"></div>
                      </div>
                    </div>
                  </div>
                  <div class="form-group vehicle-3">
                    <label for="vehicle3" class="col-sm-4 control-label">
                      Vehicle 3. Make/Model/Year
                    </label>
                    <div class="col-sm-8">
                      <div class="row">
                        <div class="col-sm-4" data-editors="vehicle3" title="Vehicle Make/Model/Year"></div>
                        <div class="col-sm-4" data-editors="license3" title="License Plate #"></div>
                        <div class="col-sm-4" data-editors="badge_number_3" title="Badge #"></div>
                      </div>
                    </div>
                  </div>
                  <div class="form-group vehicle-4">
                    <label for="vehicle4" class="col-sm-4 control-label">
                      Vehicle 4. Make/Model/Year
                    </label>
                    <div class="col-sm-8">
                      <div class="row">
                        <div class="col-sm-4" data-editors="vehicle4" title="Vehicle Make/Model/Year"></div>
                        <div class="col-sm-4" data-editors="license4" title="License Plate #"></div>
                        <div class="col-sm-4" data-editors="badge_number_4" title="Badge #"></div>
                      </div>
                    </div>
                  </div>
                  <div class="form-group vehicle-5">
                    <label for="vehicle5" class="col-sm-4 control-label">
                      Vehicle 5. Make/Model/Year
                    </label>
                    <div class="col-sm-8">
                      <div class="row">
                        <div class="col-sm-4" data-editors="vehicle5" title="Vehicle Make/Model/Year"></div>
                        <div class="col-sm-4" data-editors="license5" title="License Plate #"></div>
                        <div class="col-sm-4" data-editors="badge_number_5" title="Badge #"></div>
                      </div>
                    </div>
                  </div>
                </div>

              </div>
            </div>
          </div>
        </div>
      </div>

      <div class="panel panel-default">
        <div class="panel-heading">
          <h4 class="panel-title">
            <i class="fa fa-minus"></i> <i class="fa fa-plus"></i>
            <a data-toggle="collapse" data-parent=".accordion" href="#demographics"> Demographics </a>
          </h4>
        </div>
        <div id="demographics" class="panel-collapse collapse">
          <div class="panel-body">
            <div class="row">
              <div class="col-md-6">
                <div class="heading"> Occupant </div>
                <div data-fields="gender,birthday,last4_ssn,household_status,household_size"> </div>

                <div class="heading"> Previous Residence </div>
                <div data-fields="moving_from"> </div>

              </div>

              <div class="col-md-6">
                <div class="heading"> Employment </div>
                <div data-fields="occupation_type,employer"> </div>
                <div class="form-group">
                  <label for="employer_city" class="col-sm-4 control-label">
                    Employer City/State
                  </label>      
                  <div class="col-sm-8">
                    <div class="row">
                      <div class="col-sm-6" data-editors="employer_city"></div>
                      <div class="col-sm-6" data-editors="employer_state"></div>
                    </div>
                  </div>
                </div>

                <div data-fields="annual_income,minutes_to_work,transportation_to_work"> </div>

              </div>
            </div>

          </div>
        </div>
      </div>

    <% } else { %>

      <% if(isCreateNew){ %> <!-- org group create -->
        N/A

      <% } else {  %> <!-- org group update -->
        <div class="panel panel-default">
          <div class="panel-heading">
            <h4 class="panel-title">
              <i class="fa fa-minus"></i> <i class="fa fa-plus"></i>
              <a data-toggle="collapse" data-parent=".accordion" href="#basic-info"> Basic Information </a>
            </h4>
          </div>
          <div id="basic-info" class="panel-collapse collapse">
            <div class="panel-body" data-fields="full_name,email,primary_phone"> </div>
          </div>
        </div>

        <div class="panel panel-default">
          <div class="panel-heading">
            <h4 class="panel-title">
              <i class="fa fa-minus"></i> <i class="fa fa-plus"></i>
              <a data-toggle="collapse" data-parent=".accordion" href="#contact-info"> Contact Information </a>
            </h4>
          </div>
          <div id="contact-info" class="panel-collapse collapse">
            <div class="panel-body" data-fields="street,city,state,zip,gender,birthday,cell_phone,home_phone,work_phone"> </div>
          </div>
        </div>

      <% } %>
    <% } %>
  </div>
</form>
