var leadDialog = $('#lead-dialog'),
  leadList = "<%= j(render 'leads')%>";
  
if(leadDialog.is(':visible')){
  leadDialog.find('.modal-body').html(leadList);

} else {
  $('#audience-form .leads').html(leadList);
  $('#audience-form .leads-count').html("<%= number_with_delimiter @total_residents %> <%= @total_residents == 1 ? "Lead" : "Leads" %>");
}