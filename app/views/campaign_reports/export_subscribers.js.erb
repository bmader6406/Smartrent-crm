<% if params[:recipient] %>
  msgbox("Your data is being exported... We will email you shortly");
  
  App.exportDialog.jqmHide();
  
<% elsif @sendible %>
	window.location.href = "<%= raw request.fullpath %>";
	
<% else %>
	App.exportDialog.attr("data-url", "<%= raw request.fullpath %>").jqmShow();
	
<% end %>